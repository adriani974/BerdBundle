{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Actions</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nomaction</th>
                <td>{{ entity.nomAction }}</td>
            </tr>
            <tr>
                <th>Dateaction</th>
                <td>{{ entity.dateAction|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ entity.userId }}</td>
            </tr>
            <tr>
                <th>Idlogs</th>
                <td>{{ entity.idLogs }}</td>
            </tr>
            <tr>
                <th>Iddevice</th>
                <td>{{ entity.idDevice }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('actions') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('actions_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
