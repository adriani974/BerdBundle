{% extends '::base.html.twig' %}

{% block body -%}	
    <h1>Resultat Requete</h1>
       <tbody>
		{% for table in tabEntity %}
			<table border="2" id="table">
				<tr>
				    {% set tabKey = table[0] %}
					{% for key in tabKey %}
						    {% for k in key %}
								<td bgcolor="blue"><font color="white">{{ k }}</font></td>
							{% endfor %}
					{% endfor %}
				</tr>
				{% set tabValue = table[1] %}
				{% for value in tabValue %}
				    <tr>
						{% for v in value %}
							<td>{{ v }}</td>
						{% endfor %}
					</tr>
				{% endfor %}
			</table><br>
		{% endfor %}
       </tbody>
{% endblock %}

