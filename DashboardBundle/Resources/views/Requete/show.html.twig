{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Requete</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ entity.body }}</td>
            </tr>
            <tr>
                <th>Requestname</th>
                <td>{{ entity.requestName }}</td>
            </tr>
            <tr>
                <th>Isenable</th>
                <td>{{ entity.isEnable }}</td>
            </tr>
            <tr>
                <th>Isfixed</th>
                <td>{{ entity.isFixed }}</td>
            </tr>
            <tr>
                <th>Rendertype</th>
                <td>{{ entity.renderType }}</td>
            </tr>
            <tr>
                <th>Userid</th>
                <td>{{ entity.userId }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('requete') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('requete_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
